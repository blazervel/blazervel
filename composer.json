{
  "name": "blazervel/blazervel",
  "description": "Zero-configuration Laravel development workflow",
  "license": "MIT",
  "authors": [
      {
          "name": "Joshua Anderton",
          "email": "hey@joshuaanderton.ca"
      }
  ],
  "require": {
      "php": "^8.1",
      "illuminate/support": "^8.67|^9.0",
      "blazervel/actions": "dev-master",
      "blazervel/actionsjs": "dev-master",
      "blazervel/ui": "dev-master"
  },
  "require-dev": {
      "phpunit/phpunit": "^9.3",
      "phpstan/phpstan": "^1.7",
      "laravel/pint": "^1.1"
  },
  "autoload": {
      "psr-4": {
          "Blazervel\\Blazervel\\": "src"
      }
  },
  "autoload-dev": {
      "psr-4": {
          "Blazervel\\Blazervel\\Tests\\": "tests"
      }
  },
  "config": {
      "sort-packages": true
  },
  "extra": {
      "laravel": {
          "providers": [
              "Blazervel\\Blazervel\\Providers\\ServiceProvider"
          ]
      }
  }
}